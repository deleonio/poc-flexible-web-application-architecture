{
  "name": "poc-flexible-web-application-architecture",
  "version": "1.0.0",
  "private": true,
  "description": "This project demonstrate a flexible Babel and Webpack based web application architecture.",
  "author": {
    "name": "Martin Oppitz",
    "email": "github@martinoppitz.com",
    "url": "http://martinoppitz.com"
  },
  "scripts": {
    "build": "lean build --gzip",
    "build-all": "sh build.sh",
    "clear": "rm -rf .nyc_output coverage dist storybook-static tests_output *.log",
    "coverage": "lean coverage",
    "coveralls": "coveralls < coverage/lcov.info",
    "e2e": "lean e2e",
    "ng:serve": "ng serve",
    "ng:build": "ng build --prod",
    "reinstall": "npm run remove && npm run update",
    "remove": "rm -rf node_modules package-lock.json",
    "serve": "lean serve",
    "storybook:serve": "start-storybook",
    "storybook:build": "build-storybook",
    "test": "cross-env NODE_ENV=test mocha --config=node_modules/@leanup/cli/.mocharc.js",
    "update": "npm install && npm update",
    "vue:serve": "vue-cli-service serve src/vue.ts",
    "vue:build": "vue-cli-service build src/vue.ts --prod"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{html,js,json,jsx,ts,tsx,vue,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@angular/common": "^9.1.0",
    "@angular/compiler": "^9.1.0",
    "@angular/compiler-cli": "^9.1.0",
    "@angular/core": "^9.1.0",
    "@angular/forms": "^9.1.0",
    "@angular/platform-browser": "^9.1.0",
    "@angular/platform-browser-dynamic": "^9.1.0",
    "@leanup/features": "^1.0.0-rc.134",
    "@leanup/material-preact": "^1.0.0-rc.157",
    "@types/react": "^16.9.32",
    "@vue/composition-api": "^0.5.0",
    "angular": "^1.7.0",
    "aurelia-bootstrapper": "^2.3.0",
    "aurelia-framework": "^1.3.0",
    "core-js": "^3.6.0",
    "inferno": "^7.4.2",
    "navigo": "^7.1.0",
    "preact": "^10.3.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "register-service-worker": "^1.7.1",
    "svelte": "^3.20.1",
    "vue": "^2.6.0",
    "vue-template-compiler": "^2.6.0",
    "vuetify": "^2.2.20",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.0",
    "@angular/cli": "^9.1.0",
    "@leanup/cli": "^1.0.0-rc.175",
    "@leanup/cli-addons": "^1.0.0-rc.173",
    "@leanup/cli-angular": "^1.0.0-rc.173",
    "@leanup/cli-angularjs": "^1.0.0-rc.173",
    "@leanup/cli-aurelia": "^1.0.0-rc.173",
    "@leanup/cli-inferno": "^1.0.0-rc.173",
    "@leanup/cli-preact": "^1.0.0-rc.173",
    "@leanup/cli-react": "^1.0.0-rc.173",
    "@leanup/cli-svelte": "^1.0.0-rc.173",
    "@leanup/cli-vue": "^1.0.0-rc.173",
    "@vue/cli": "^4.2.3",
    "@vue/cli-service": "^4.2.3",
    "chromedriver": "^80.0.0",
    "coveralls": "^3.0.11",
    "eslint-plugin-prettier": "^3.1.2",
    "geckodriver": "^1.19.0",
    "graphql": "^15.0.0",
    "husky": "^4.2.3",
    "lerna": "^3.20.2",
    "lint-staged": "^10.1.1",
    "sass-convert": "^0.5.0",
    "sass-lint": "^1.13.0",
    "selenium-server": "^3.141.59",
    "typescript": "~3.8.3",
    "workbox-cli": "^5.1.2"
  }
}
