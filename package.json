{
  "name": "poc-flexible-web-application-architecture",
  "version": "1.0.0",
  "private": true,
  "description": "This project demonstrate a flexible Babel and Webpack based web application architecture.",
  "author": {
    "name": "Martin Oppitz",
    "email": "github@martinoppitz.com",
    "url": "http://martinoppitz.com"
  },
  "scripts": {
    "e2e-test": "mkdirp .reports && cucumber-js tests/cucumber/features/**/*.feature --require cucumber.conf.js --require tests/cucumber/step-definitions --format node_modules/cucumber-pretty --format json:.reports/cucumber.json",
    "build": "lean build --gzip",
    "build-all": "sh build.sh",
    "clear": "rm -rf .nyc_output coverage dist storybook-static tests_output *.log",
    "coverage": "lean coverage",
    "coveralls": "coveralls < coverage/lcov.info",
    "e2e": "lean e2e",
    "ng:serve": "ng serve",
    "ng:build": "ng build --prod",
    "reinstall": "npm run remove && npm run update",
    "remove": "rm -rf node_modules package-lock.json",
    "serve": "lean serve --framework preact",
    "start": "npm run serve -- --open",
    "storybook:serve": "start-storybook",
    "storybook:build": "build-storybook",
    "test": "cross-env NODE_ENV=test mocha --config=node_modules/@leanup/cli/.mocharc.js",
    "update": "npm install && npm update",
    "vue:serve": "vue-cli-service serve src/vue.ts",
    "vue:build": "vue-cli-service build src/vue.ts --prod"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "{src,tests}/**/*.{html,js,json,jsx,ts,tsx,gql,graphql}": [
      "eslint --fix"
    ],
    "{src,tests}/**": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@angular/common": "^9.1.6",
    "@angular/compiler": "^9.1.6",
    "@angular/compiler-cli": "^9.1.6",
    "@angular/core": "^9.1.6",
    "@angular/forms": "^9.1.6",
    "@angular/platform-browser": "^9.1.6",
    "@angular/platform-browser-dynamic": "^9.1.6",
    "@leanup/features": "^1.0.0-rc.217",
    "@leanup/material-preact": "^1.0.0-rc.217",
    "@types/react": "^16.9.34",
    "@vue/composition-api": "^0.5.0",
    "angular": "^1.7.0",
    "aurelia-bootstrapper": "^2.3.0",
    "aurelia-framework": "^1.3.0",
    "inferno": "^7.4.2",
    "navigo": "^7.1.0",
    "preact": "^10.4.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "register-service-worker": "^1.7.1",
    "svelte": "^3.22.2",
    "vue": "^2.6.0",
    "vue-template-compiler": "^2.6.0",
    "vuetify": "^2.2.27",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.5",
    "@angular/cli": "^9.1.5",
    "@leanup/cli": "^1.0.0-rc.219",
    "@leanup/cli-addons": "^1.0.0-rc.204",
    "@leanup/cli-angular": "^1.0.0-rc.219",
    "@leanup/cli-angularjs": "^1.0.0-rc.204",
    "@leanup/cli-aurelia": "^1.0.0-rc.204",
    "@leanup/cli-inferno": "^1.0.0-rc.204",
    "@leanup/cli-preact": "^1.0.0-rc.204",
    "@leanup/cli-react": "^1.0.0-rc.204",
    "@leanup/cli-svelte": "^1.0.0-rc.217",
    "@leanup/cli-vue": "^1.0.0-rc.217",
    "@leanup/git-hooks": "^1.0.0-rc.217",
    "@vue/cli": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "chromedriver": "^81.0.0",
    "coveralls": "^3.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "geckodriver": "^1.19.0",
    "graphql": "^15.0.0",
    "lerna": "^3.20.2",
    "sass-convert": "^0.5.0",
    "sass-lint": "^1.13.0",
    "selenium-server": "^3.141.59",
    "typescript": "~3.8.3",
    "workbox-cli": "^5.1.3"
  }
}
