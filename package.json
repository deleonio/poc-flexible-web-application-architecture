{
  "name": "poc-flexible-web-application-architecture",
  "version": "1.0.0",
  "private": true,
  "description": "This project demonstrate a flexible Babel and Webpack based web application architecture.",
  "author": {
    "name": "Martin Oppitz",
    "email": "github@martinoppitz.com",
    "url": "http://martinoppitz.com"
  },
  "scripts": {
    "build": "webpack --mode=production --env.framework",
    "build-all": "yarn build angular && yarn build angularjs && yarn build inferno && yarn build preact && yarn build react && yarn build svelte && yarn build vue",
    "clear": "rm -rf .nyc_output coverage dist tests_output *.log",
    "coverage": "cross-env NODE_ENV=test nyc mocha",
    "e2e": "nightwatch",
    "prettier": "prettier --write src/**/*.ts*",
    "reinstall": "rm -rf node_modules && yarn install",
    "serve": "webpack-dev-server --colors --devtool source-map --progress --env.framework",
    "storybook:serve": "start-storybook",
    "storybook:build": "build-storybook",
    "test": "cross-env NODE_ENV=test mocha --watch-extensions ts",
    "vue:serve": "vue-cli-service serve src/vue.ts",
    "vue:build": "vue-cli-service build src/vue.ts",
    "update": "yarn && yarn upgrade && yarn outdated"
  },
